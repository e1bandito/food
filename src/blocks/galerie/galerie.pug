- let images = ['event-1', 'event-2', 'event-3', 'event-4']

+b.SECTION.galerie#galerie
  +e.H2.title.visually-hidden Galerie
  .container
    +e.UL.list
      each item in images
        +e.LI.item
          +e.image
            a(data-fslightbox, href="img/"+ item +"@2x.jpg")
              +picture(item, item, 'jpg')
